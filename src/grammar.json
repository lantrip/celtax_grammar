{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "celtax",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "line"
          },
          {
            "type": "SYMBOL",
            "name": "newline"
          }
        ]
      }
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "file_header"
        },
        {
          "type": "SYMBOL",
          "name": "act_header"
        },
        {
          "type": "SYMBOL",
          "name": "scene_header"
        },
        {
          "type": "SYMBOL",
          "name": "cel_header"
        },
        {
          "type": "SYMBOL",
          "name": "content_type_beat"
        },
        {
          "type": "SYMBOL",
          "name": "content_type_treatment"
        },
        {
          "type": "SYMBOL",
          "name": "content_type_narrative"
        },
        {
          "type": "SYMBOL",
          "name": "entity_definition"
        },
        {
          "type": "SYMBOL",
          "name": "entity_block_start"
        },
        {
          "type": "SYMBOL",
          "name": "entity_block_end"
        },
        {
          "type": "SYMBOL",
          "name": "import_statement"
        },
        {
          "type": "SYMBOL",
          "name": "adapter_statement"
        },
        {
          "type": "SYMBOL",
          "name": "metadata_line"
        },
        {
          "type": "SYMBOL",
          "name": "prose_line"
        }
      ]
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "file_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[A-Z][A-Z_]*"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "act_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n(]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\(\\d+(?:\\.\\d+)?%\\)"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "scene_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n(]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\(\\d+(?:\\.\\d+)?%\\)"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "cel_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "==="
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n-]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "-"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n-]+"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "-"
                        },
                        {
                          "type": "PATTERN",
                          "value": "[^\\r\\n()]+"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\(\\d+(?:\\.\\d+)?%\\)"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "content_type_beat": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "///"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "content_type_treatment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "content_type_narrative": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "entity_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": "\\w+"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "PATTERN",
          "value": "[A-Za-z0-9][A-Za-z0-9\\s]*[A-Za-z0-9]|[A-Za-z0-9]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "PATTERN",
                  "value": ".*"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "entity_block_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": "\\w+s?"
        },
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "STRING",
          "value": "{"
        }
      ]
    },
    "entity_block_end": {
      "type": "STRING",
      "value": "}"
    },
    "import_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@import"
        },
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "PATTERN",
                  "value": "[^\"]+"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "'"
                },
                {
                  "type": "PATTERN",
                  "value": "[^']+"
                },
                {
                  "type": "STRING",
                  "value": "'"
                }
              ]
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\s+as\\s+"
                },
                {
                  "type": "PATTERN",
                  "value": "\\w+"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ")"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\s*#"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]+"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "adapter_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@adapter"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "\""
                        },
                        {
                          "type": "PATTERN",
                          "value": "[^\"]+"
                        },
                        {
                          "type": "STRING",
                          "value": "\""
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "'"
                        },
                        {
                          "type": "PATTERN",
                          "value": "[^']+"
                        },
                        {
                          "type": "STRING",
                          "value": "'"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "PATTERN",
                          "value": "\\s+@timing:"
                        },
                        {
                          "type": "PATTERN",
                          "value": "\\w+"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\w+"
                },
                {
                  "type": "PATTERN",
                  "value": "\\s*"
                },
                {
                  "type": "STRING",
                  "value": "{"
                }
              ]
            }
          ]
        }
      ]
    },
    "metadata_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": "\\w+"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "prose_line": {
      "type": "PATTERN",
      "value": "[^\\r\\n=@/#][^\\r\\n]*"
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}